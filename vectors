#include<bits/stdc++.h>
using namespace std;
int main()
{
	vector<int>ve;
	vector<int>::iterator it;
	for(int i=0;i<5;i++)
	{
		ve.push_back(i*2);
	}
	for(it=ve.begin();it!=ve.end();it++)

	{
		cout<<*it<<"   ";
	}
	ve.pop_back(); //remove last number

	cout<<endl;
	for(it=ve.begin();it!=ve.end();it++)

	{
		cout<<*it<<"   ";
	}
	ve.erase(ve.begin()+1,ve.begin()+3);//delete certain numbers
	cout<<endl;
	for(it=ve.begin();it!=ve.end();it++)

	{
		cout<<*it<<"   ";
	}
	cout<<endl;
	ve.insert(ve.begin(),300);//300 is insserted at begining
	ve.insert(ve.end(),200);//insert 200 at ending
	for(it=ve.begin();it!=ve.end();it++)

	{
		cout<<*it<<"   ";
	}
	cout<<endl;
	sort(ve.begin(),ve.end());//to sort the vectorfor(it=ve.begin();it!=ve.end();it++)

	{
		cout<<*it<<"   ";
	}
	for(it=ve.begin();it!=ve.end();it++)

	{
		cout<<*it<<"   ";
	}

}
